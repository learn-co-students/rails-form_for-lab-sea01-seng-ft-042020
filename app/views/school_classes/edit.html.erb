<h1>SchoolClasses#edit</h1>
<p>Find me in app/views/school_classes/edit.html.erb</p>
<h1>Edit School Class Form:</h1>
<%= form_for(@school_class) do |t|%>
    <%= t.label(:title)%>
    <%= t.text_field(:title)%>
    <%= t.label(:room_number)%>
    <%= t.text_field(:room_number)%>
    <%= t.submit("Update School class")%>
<% end %>